<script>
    document.addEventListener('DOMContentLoaded', function() {
        let BottonFB = document.querySelector('.BottonFB');
        let modalDialog = document.querySelector('.modalDialog');
        let contenedor = document.querySelector('.contenedorW');
        let close = document.querySelector('.close');

        if (BottonFB && modalDialog && contenedor && close) {
            BottonFB.onclick = function() {
                modalDialog.classList.add('active');
                contenedor.classList.add('active');
            }

            close.onclick = function() {
                modalDialog.classList.remove('active');
                contenedor.classList.remove('active');
            }
        }
    });
</script>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        const messageContainer = document.querySelector('.messageContainer');
        if (messageContainer) {
            setTimeout(() => {
                messageContainer.classList.add('active');
            }, 500); // Ajusta el tiempo de espera según tu preferencia
        }
